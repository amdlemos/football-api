<?xml version="1.0"?>
<psalm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://getpsalm.org/schema/config" errorLevel="8" resolveFromConfigFile="true" xsi:schemaLocation="https://getpsalm.org/schema/config vendor/vimeo/psalm/config.xsd" findUnusedBaselineEntry="true" findUnusedCode="true">
  <projectFiles>
    <directory name="app"/>
    <directory name="database/factories"/>
    <directory name="database/seeders"/>
    <ignoreFiles>
      <directory name="vendor"/>
    </ignoreFiles>
  </projectFiles>
  <issueHandlers>
    <UnusedVariable errorLevel="suppress"/>
    <UndefinedConstant errorLevel="suppress"/>
    <UndefinedMagicMethod errorLevel="suppress"/>
    <LessSpecificImplementedReturnType errorLevel="suppress"/>
  </issueHandlers>
</psalm>
